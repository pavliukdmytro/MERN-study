  "bugs": {
    "url": "https://github.com/pavliukdmytro/MERN-study/issues"
  },
  "homepage": "https://github.com/pavliukdmytro/MERN-study#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.2.5",
    "cross-env": "^7.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.1",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
